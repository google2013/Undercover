    <color name="bluebtn1">#90d8e8</color>
    <color name="bluebtn2">#52c2db</color>
    <color name="graybtn">#dfe3e4</color>
    <color name="graybtn2">#d8dede</color>
<color name="WRITE">#ffffff</color>
